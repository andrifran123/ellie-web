{
  "rewrites": [
    { "source": "/api/ws/(.*)", "destination": "https://ellie-api-1.onrender.com/ws/$1" },
    { "source": "/api/(.*)", "destination": "https://ellie-api-1.onrender.com/api/$1" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Frame-Options", "value": "SAMEORIGIN" },
        { "key": "Referrer-Policy", "value": "strict-origin-when-cross-origin" }
      ]
    }
  ]
}
